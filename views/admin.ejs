<head>
  <meta charset="UTF-8">
  <title>SentinelSync</title>
  <!-- Bootstrap -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
</head>


<h2>Admin Dashboard</h2>
<table border="1">
  <tr><th>ID</th><th>User</th><th>Role</th><th>Date</th><th>Event</th><th>Description</th></tr>
  <% reports.forEach(r => { %>
    <tr>
      <td><%= r.reportID %></td>
      <td><%= r.userID %></td>
      <td><%= r.reporterRole %></td>
      <td><%= r.dateTime %></td>
      <td><%= r.eventType %></td>
      <td><%= r.description %></td>
    </tr>
  <% }) %>
</table>
